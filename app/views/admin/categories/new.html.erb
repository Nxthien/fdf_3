<% provide :title, t("create.title_cate") %>
<h1><%= t "create.title_cate" %></h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @category] do |f| %>
    
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>

      <%= f.submit t("create.button"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
